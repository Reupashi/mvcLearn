@model IEnumerable<ClassroomsInfo.Web.Models.ClassroomTableModel>

@{
    ViewBag.Title = "Selection";
}

    <div class="row panel">
        <div id="parameters" class="col-sm-3">
            <h3>Параметри</h3>
            @Html.Partial("_SelectionForm")
        </div>
        <div class="col-sm-10">
            <table class="table">
                <tbody id="data">
                    @Html.Partial("_TableBody", @Model)
                </tbody>
            </table>
        </div>
    </div>
